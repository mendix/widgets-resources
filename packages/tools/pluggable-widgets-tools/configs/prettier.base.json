{
  "trailingComma": "none",
  "useTabs": false,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": false,
  "printWidth": 120,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "xmlSelfClosingSpace": true,
  "xmlWhitespaceSensitivity": "ignore"
}
