{
    "name": "custom-widgets-a11y-tests",
    "version": "1.0.0",
    "description": "A11y tests for custom widgets, using axe",
    "main": "index.js",
    "scripts": {
        "test": "wdio",
        "test:debug": "cross-env DEBUG=true wdio",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "ci": "npm run lint && wdio",
        "lint": "concurrently \"tsc\" \"npm run lint:eslint\" \"npm run lint:prettier\"",
        "lint:eslint": "eslint \"src/**/*.ts\" --max-warnings 0",
        "lint:prettier": "prettier -l \"src/**/*.ts\""
    },
    "author": "WT",
    "license": "ISC",
    "dependencies": {
        "@types/jasmine": "^3.4.0",
        "@types/webdriverio": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^2.8.0",
        "@typescript-eslint/parser": "^2.8.0",
        "@wdio/cli": "^5.10.9",
        "@wdio/dot-reporter": "^5.9.3",
        "@wdio/jasmine-framework": "^5.10.8",
        "@wdio/junit-reporter": "^5.15.5",
        "@wdio/local-runner": "^5.10.9",
        "@wdio/selenium-standalone-service": "^5.9.3",
        "@wdio/spec-reporter": "^5.9.3",
        "@wdio/sync": "^5.11.0",
        "axe-core": "^3.4.0",
        "concurrently": "^5.0.0",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^23.0.4",
        "eslint-plugin-promise": "^4.2.1",
        "prettier": "^1.19.1",
        "ts-node": "^8.3.0",
        "typescript": "^3.6.2",
        "webdriverio": "^5.10.9"
    }
}
