    "react-native-image-picker": "2.3.1",
    "react-native-inappbrowser-reborn": "3.3.4",
    "react-native-localize": "1.3.4",
    "react-native-sound": "0.11.0",
    "react-native-touch-id": "4.4.1"
  },
  "devDependencies": {
    "@types/querystringify": "^2.0.0",
    "@types/url-parse": "^1.4.3"
  }
