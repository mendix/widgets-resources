{
  "name": "progressbar",
  "widgetName": "ProgressBar",
  "version": "1.1.0",
  "description": "Displays a progress bar",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --watch Gruntfile.js --exec grunt",
    "pretest": "tsc && npm run lint && grunt release",
    "test": "npm run test:unit",
    "test:dev": "karma start",
    "test:unit": "karma start --single-run --codeCoverage && node tests/remap.js",
    "pack": "webpack",
    "emit": "tsc",
    "lint": "tslint -c tslint.json '**/*.ts' --exclude '**/node_modules/**' --exclude '**/dist/**'"
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mendixlabs/progress-bar"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mendixlabs/progress-bar/issues"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.4",
    "@types/classnames": "^2.2.3",
    "@types/core-js": "^0.9.42",
    "@types/dojo": "^1.9.38",
    "@types/enzyme": "^3.1.10",
    "@types/jasmine": "^2.6.0",
    "@types/karma": "^1.7.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "check-dependencies": "^1.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "grunt": "^1.0.2",
    "grunt-check-dependencies": "^1.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.4.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.1",
    "grunt-file-append": "0.0.7",
    "grunt-newer": "^1.1.1",
    "grunt-webpack": "^3.1.1",
    "istanbul": "^0.4.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "json-loader": "^0.5.4",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
