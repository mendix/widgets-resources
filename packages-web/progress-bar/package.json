    "mendix-client": "^7.13.3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.2",
    "remap-istanbul": "^0.11.1",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.2.0",
