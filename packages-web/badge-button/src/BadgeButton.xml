<?xml version="1.0" encoding="utf-8"?>
<widget id="com.mendix.widget.web.badgebutton.BadgeButton" pluginWidget="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../../../node_modules/mendix/custom_widget.xsd">
    <name>Badge button v2</name>
    <description>A special distinctive mark or token put on any display as a button</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General">
            <propertyGroup caption="Button">
                <property key="label" type="textTemplate" required="false">
                    <caption>Caption</caption>
                    <description>The caption of the button</description>
                    <translations>
                        <translation lang="en_US">Button</translation>
                        <translation lang="nl_NL">Button</translation>
                    </translations>
                </property>
                <property key="bootstrapStyle" type="enumeration" defaultValue="default">
                    <caption>Button style</caption>
                    <description>The appearance of the badge button</description>
                    <enumerationValues>
                        <enumerationValue key="default">Default</enumerationValue>
                        <enumerationValue key="primary">Primary</enumerationValue>
                        <enumerationValue key="success">Success</enumerationValue>
                        <enumerationValue key="info">Info</enumerationValue>
                        <enumerationValue key="warning">Warning</enumerationValue>
                        <enumerationValue key="danger">Danger</enumerationValue>
                    </enumerationValues>
                </property>
            </propertyGroup>
            <propertyGroup caption="Badge">
                <property key="value" type="textTemplate" required="false">
                    <caption>Badge value attribute</caption>
                    <description>The attribute that contains the badge button value</description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Events">
                <property key="onClickEvent" type="action" required="false">
                    <caption>On click</caption>
                    <description />
                </property>
            </propertyGroup>
        </propertyGroup>
    </properties>
</widget>
