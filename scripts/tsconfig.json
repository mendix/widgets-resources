{
    "include": ["./release", "./types", "../node_modules/gh-release"],
    "compilerOptions": {
        "strict": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["node"],
        "esModuleInterop": true
    }
}
